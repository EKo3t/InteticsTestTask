@model Pharmacy.Models.ClientListModel

<table class="table">
    <tr>
        <th>
            First Name
        </th>
        <th>
            Last Name
        </th>
        <th>
            Email
        </th>
        <th>
            Actions
        </th>
    </tr>

    @foreach (var item in Model.ClientList)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.FirstName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LastName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
            <td>
                @Html.ActionLink("Details", "Details", "Client", item, null) |
                @Html.ActionLink("Visit List", "VisitList", "Client", new { item.id }, null)
            </td>
        </tr>
    }

</table>